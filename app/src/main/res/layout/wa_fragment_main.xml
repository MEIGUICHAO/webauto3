<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >


    <com.example.webtest.widget.MyScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:id="@+id/tv_learning"
                android:background="@color/colorPrimary"
                android:text="learning"
                android:padding="2dp"
                android:visibility="gone"
                android:textColor="@android:color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/et_no"
                    android:hint="sc"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:inputType="number"
                    android:textSize="18sp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_date"
                    android:hint="date"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:textSize="18sp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_caculate"
                    android:text="1"
                    android:hint="caculate"
                    android:inputType="number"
                    android:visibility="gone"

                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:textSize="18sp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_rise"
                    android:text="+"
                    android:visibility="gone"

                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_discount"
                    android:text="-"
                    android:layout_weight="2"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_change_lottory"
                    android:text="change"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:visibility="gone"
                android:layout_height="wrap_content">

                <EditText
                    android:hint="bigger"
                    android:id="@+id/et_bigger"
                    android:inputType="number"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_normal"
                    android:text="normal"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_big"
                    android:text="big"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_useful"
                    android:text="useful"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <EditText
                    android:hint="mutiple"
                    android:id="@+id/et_10mutiple"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="10+ smaller"
                    android:id="@+id/et_10none"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <EditText
                    android:hint="20+ smaller"
                    android:id="@+id/et_20none"
                    android:visibility="gone"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="none"
                    android:id="@+id/et_none"
                    android:layout_weight="3"
                    android:inputType="number"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="dif"
                    android:id="@+id/et_dif"
                    android:layout_weight="3"
                    android:inputType="number"
                    android:text="35"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="same"
                    android:id="@+id/et_same"
                    android:text="23"
                    android:layout_weight="3"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <EditText
                    android:hint="blank"
                    android:id="@+id/et_blank"
                    android:layout_weight="4"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <Button
                    android:id="@+id/btn_get"
                    android:text="get"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_caculate"
                    android:text="caculate"
                    android:layout_weight="5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <EditText
                    android:id="@+id/et_allcount"
                    android:hint="count"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:inputType="number"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_blank20"
                    android:hint="b20"
                    android:inputType="number"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_blank15"
                    android:hint="b15"
                    android:inputType="number"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_blank10"
                    android:hint="b10"
                    android:inputType="number"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_custom"
                    android:text="@string/custom"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />


            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <Button
                    android:id="@+id/btn_change"
                    android:text="change"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:visibility="gone"
                    android:layout_height="wrap_content" />


                <EditText
                    android:hint="urlNum"
                    android:inputType="number"
                    android:id="@+id/et_less"
                    android:layout_weight="1"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content" />


                <Button
                    android:id="@+id/btn_reset"
                    android:text="reseturl"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_resetdata"
                    android:text="set"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content" />


                <Button
                    android:id="@+id/btn_learn"
                    android:text="@string/AutoDeal"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_check"
                    android:text="FcAuto"
                    android:visibility="gone"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <EditText
                    android:id="@+id/et_amount"
                    android:text="1"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />


                <Button
                    android:id="@+id/btn_amount_rise"
                    android:text="+"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <Button
                    android:id="@+id/btn_amount_discount"
                    android:text="-"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <Button
                    android:id="@+id/btn_sc"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text="goSc" />

                <Button
                    android:id="@+id/btn_ft"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text="goFt" />

                <Button
                    android:id="@+id/btn_go"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text="go!!" />

            </LinearLayout>

            <TextView
                android:id="@+id/tv_allcount"
                android:textColor="@android:color/holo_orange_dark"
                android:padding="8dp"
                android:textSize="14sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_result"
                android:padding="8dp"
                android:textSize="14sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_learn_result2"
                android:padding="8dp"
                android:textSize="14sp"
                android:textColor="@android:color/holo_blue_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_learn_result3"
                android:padding="8dp"
                android:textSize="14sp"
                android:textColor="@android:color/holo_orange_light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_learn_result"
                android:padding="8dp"
                android:textSize="14sp"
                android:textColor="@color/red_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <WebView
                android:id="@+id/wa_webview_list"
                android:layout_width="fill_parent"
                android:layout_height="600dp" />
        </LinearLayout>
    </com.example.webtest.widget.MyScrollView>



    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal" >

        <WebView
            android:id="@+id/wa_webview_detail"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1" />
    </LinearLayout>

</LinearLayout>